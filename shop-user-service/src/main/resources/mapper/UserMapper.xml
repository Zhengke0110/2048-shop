<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="fun.timu.shop.user.mapper.UserMapper">

    <resultMap id="BaseResultMap" type="fun.timu.shop.user.model.DO.UserDO">
        <id property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="pwd" column="pwd"/>
        <result property="headImg" column="head_img"/>
        <result property="slogan" column="slogan"/>
        <result property="sex" column="sex"/>
        <result property="points" column="points"/>
        <result property="createTime" column="create_time"/>
        <result property="mail" column="mail"/>
        <result property="secret" column="secret"/>
    </resultMap>

    <sql id="Base_Column_List">
        id
        ,name,pwd,head_img,slogan,sex,
        points,create_time,mail,secret
    </sql>
</mapper>
